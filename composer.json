{
    "name": "tiendagrupo5/ecommerce",
    "description": "Sistema de tienda online con POO en PHP",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "fakerphp/faker": "^1.20"
    },
    "autoload": {
        "psr-4": {
